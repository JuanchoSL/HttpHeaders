{
    "name": "juanchosl/httpheaders",
    "description": "Little methods collection in order to create http headers",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/JuanchoSL/HttpHeaders",
    "minimum-stability": "stable",
    "keywords": [
        "headers",
        "http",
        "file extensions",
        "mime types",
        "content types"
    ],
    "authors": [
        {
            "name": "Juan SÃ¡nchez Lecegui",
            "email": "JuanchoSL@hotmail.com",
            "homepage": "https://github.com/JuanchoSL/"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.6.*",
        "phpstan/phpstan": "1.10.*"
    },
    "autoload": {
        "psr-4": {
            "JuanchoSL\\HttpHeaders\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JuanchoSL\\HttpHeaders\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit --configuration phpunit.xml tests",
        "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon --xdebug"
    }
}